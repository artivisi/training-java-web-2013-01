<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
        
    <servlet>
        <servlet-name>SimpanLpk</servlet-name>
        <servlet-class>com.artivisi.training.web.servlet.SimpanLpkServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>SimpanLpk</servlet-name>
        <url-pattern>/simpan-lpk.php</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>aplikasi-lpk</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>aplikasi-lpk</servlet-name>
        <url-pattern>/aplikasi-springmvc/*</url-pattern>
    </servlet-mapping>
    
</web-app>
